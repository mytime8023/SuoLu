<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.suolu.dao.PicDao"> 
<insert id="addPic" parameterType="java.util.Map">
      insert into picadd (picname,sxname,piczz,picadd) VALUES (#{picname},#{sxname},#{piczz},#{picadd})  
 </insert>
 <select id="getPic" resultType="java.util.Map"  >
     select * from picadd    
 </select>
</mapper>
